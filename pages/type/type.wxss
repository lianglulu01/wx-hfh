/* pages/type/type.wxss */
page{background-color: #f4f4f4}
.type{
  padding: 20rpx;
  margin-bottom: 20rpx;
  background-color: white;
  color:#555;
}
.parenttitle{
  padding: 10rpx 0 24rpx;
  border-bottom: 1px solid #dcdcdc;
  font-size: 26rpx
}
.parent{
  padding-top: 16rpx;
  line-height: 80rpx
}
.parent text{
  display: inline-block;
  height:60rpx;
  line-height: 60rpx;
  background-color: white;
  border-style: solid;
  border-width: 1px;
  border-color:#dcdcdc;
  border-radius: 8rpx;
  margin-right:20rpx;
  padding:0 20rpx;
  font-size: 28rpx;
  font-weight: 400;
}